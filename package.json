{
  "name": "MyBooking",
  "version": "0.0.16",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "test:deep-linking:app": "npx uri-scheme open mybooking://root/profile --android",
    "test:deep-linking:web": "npx uri-scheme open https://www.amazinghorse.io/locations/3 --android",
    "build:android": "yarn android --variant=release",
    "postbuild:android": "yarn version --patch --no-commit-hooks",
    "postversion": "react-native-version",
    "i18n:extract": "formatjs extract \"src/**/*.{js,jsx,ts,tsx}\" --ignore=\"**/*.d.ts\" --format simple --id-interpolation-pattern [sha512:contenthash:base64:6] --out-file generated/locales/ru.json",
    "i18n:compile": "formatjs compile-folder --ast --format simple generated/locales assets/locales"
  },
  "dependencies": {
    "@faker-js/faker": "^6.2.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "babel-plugin-formatjs": "^10.3.24",
    "effector": "^22.3.0",
    "effector-react": "^22.0.6",
    "intl": "^1.2.5",
    "lodash.shuffle": "^4.2.0",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "react": "17.0.2",
    "react-intl": "^6.0.4",
    "react-native": "0.68.2",
    "react-native-ico-flags": "^3.3.1",
    "react-native-pager-view": "^5.4.24",
    "react-native-paper": "^4.12.1",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.1.0",
    "semver": "^7.3.7",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@formatjs/cli": "^5.0.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/node": "^18.0.0",
    "@types/react-native": "^0.68.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-effector": "^0.7.6",
    "eslint-plugin-formatjs": "^3.1.5",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-native": "^4.0.0",
    "metro-react-native-babel-preset": "^0.70.3",
    "npm-run-all": "^4.1.5",
    "react-native-version": "^4.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
